{
  "manifest_version": 2,
  "name": "WASM Sample",
  "version": "1.3",
  "description": "A basic Chrome extension using a simple WASM file.",
  "icons": { "128": "icon_128.png" },
    "background": {
    "persistent": false,
    "scripts": ["main.js"]
  },
  "permissions": [
    "http://*/", "https://*/"
    ],
  "web_accessible_resources": [
        "content_script.js",
	"main.js",
	"simple.wasm"
   ],
  "browser_action": {	
    "default_icon":  {
	"19": "icon_19.png"
	},
	"default_title": ""
  }
}
