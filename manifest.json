{
  "manifest_version": 2,
  "name": "WASM Sample",
  "version": "1.0",
  "description": "A basic Chrome extension using a simple WASM file.",
  "icons": { "128": "icon_128.png" },

    "content_scripts": [
    {
      "matches": ["http://*/", "https://*/"],
      "all_frames": false,
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
   "activeTab"
    ],
	"web_accessible_resources": [
        "content_script.js",
	"simple.wasm"
   ],
  "browser_action": {
    "default_title": "",
    "default_icon": "icon_19.png",
    "default_popup": ""
  }
}
